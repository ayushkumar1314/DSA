#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int k;
	cin>>k;
	k=k%n;
	int arr[100];
	for(int i=0;i<n;i++)
	cin>>arr[i];
	int i=0,j=n-1;
	while(i<j){
	    swap(arr[i],arr[j]);
	    i++;j--;
	}
	i=0;j=k-1;
	while(i<j){
	    swap(arr[i],arr[j]);
	    i++;j--;
	}
	i=k,j=n-1;
	while(i<j){
	    swap(arr[i],arr[j]);
	    i++;j--;
	}
	for(int i=0;i<n;i++)
	cout<<arr[i]<<" ";

}
