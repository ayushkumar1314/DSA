#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[100];
	int max1=INT_MIN;
	for(int i=0;i<n;i++)
	cin>>arr[i];
	int pre[100]={0};
	pre[0]=arr[0];
	for(int i=1;i<n;i++)
	pre[i]=pre[i-1]+arr[i];
	for(int i=0;i<n;i++){
	    for(int j=i;j<n;j++){
	       max1=max(max1,pre[j]-(i>0?pre[i-1]:0));
	    }
	}
   cout<<max1;
}
