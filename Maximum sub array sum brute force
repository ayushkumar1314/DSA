#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[100];
	int max1=INT_MIN;
	for(int i=0;i<n;i++)
	cin>>arr[i];
	for(int i=0;i<n;i++){
	    for(int j=i;j<n;j++){
	        int x=0;
	        for(int k=i;k<=j;k++){
	            x=x+arr[k];
	        }
	       max1=max(x,max1);
	        
	    }
	}
   cout<<max1;
}
