#include <bits/stdc++.h>

using namespace std;

int main() {
    int n;
    cin >> n;
    int target;
    cin >> target;
    int index = -1;
    int arr[10];
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    for (int i = 0; i < n; i++) {
        if (arr[i] == target) {
            index = i;
            cout<<i<<" ";
            
        }
    }
    if(index==-1)
    cout << index;

}
