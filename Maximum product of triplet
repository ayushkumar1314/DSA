#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[100];
	for(int i=0;i<n;i++)
	cin>>arr[i];
	int fl=INT_MIN,sl=INT_MIN,tl=INT_MIN;
	for(int i=0;i<n;i++){
	    if(arr[i]>fl){
	        tl=sl;sl=fl;fl=arr[i];
	    }
	    else if(arr[i]>sl){
	        tl=sl;
	        sl=arr[i];
	    }
	    else if(arr[i]>tl)
	    tl=arr[i];
	}
	int fm=INT_MAX,sm=INT_MAX;
	for(int i=0;i<n;i++){
	    if(fm>arr[i]){
	        sm=fm;fm=arr[i];
	    }
	    else if(sm>arr[i])
	    sm=arr[i];
	}
	cout<<max(fl*sl*tl,fm*sm*fl);

}
